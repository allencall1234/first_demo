.wrapper {
  display: flex;
  background: #fff;
  width: 100%;
  height: 100%;
}

.left-wrapper {
  width: 30%;
  height: 100%;
  background: #ccc;
  border-right: 1px solid red;
}

.left-item {
  font-size: 25rpx;
  color: black;
  text-align: center;
}

.wrap {
  display: flex;
  background: #f8f8f8;
  width: 100%;
  height: 100%;
}

.navLeft {
  width: 25%;
  /*border-right: 1px solid #ccc; */
  background: #fff;
  height: 100%;
}

.navLeft text {
  display: block;
  font-size: 25rpx;
  text-align: center;
  height: fit-content;
  padding-left: 10rpx;
  padding-right: 10rpx;
  min-height: 80rpx;
}

.navLeft .active {
  box-sizing: border-box;
  color: #009944;
}

.navRight {
  width: 80%;
  height: 100%;
}

.navRight-main-item {
  width: 100%;
  height: 100%;
  display: flex;
}

.navRight-main-item-list {
  width: 100%;
  height: 200rpx;
  display: -webkit-flex;
  flex-wrap: wrap;
}

.navRight-main-item-list navigator {
  text-align: center;
  width: 50%;
}

.navRight-main-item-list navigator image {
  width: 80px;
  height: 60px;
  margin-top: 10px;
  box-shadow: 1px 1px 1px #ccc;
}

.navRight-main-item-list text {
  display: block;
  width: 100%;
  height: 20px;
  align-content: center;
  font-size: 12px;
  color: #666;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-bottom: 10px;
  white-space: nowrap;
}
